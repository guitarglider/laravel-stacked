  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Acme\\Stacked\\": "src/"
    }
  },
  "require": {
    "php": ">=8.2",
    "laravel/framework": "^12.0",
    "laravel/breeze": "^2.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Acme\\Stacked\\LaravelStackedServiceProvider"
      ]
    }
  }
}
